# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Base Camp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Base Camp
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'

  target 'Base CampTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Analytics'
    pod 'Firebase/Auth'
  end

  target 'Base CampUITests' do
    # Pods for testing
    pod 'Firebase/Analytics'
    pod 'Firebase/Auth'
  end

end
